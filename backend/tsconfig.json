{
    "compilerOptions": {
        "module": "CommonJS",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "target": "ES2020",
        "noImplicitAny": true,
        "sourceMap": false,
        "outDir": "dist",
    },
    "paths": {
        "*": [
            "node_modules/*",
            "src/models/*"
        ]
    },
    "include": ["src/**/*"],
}
